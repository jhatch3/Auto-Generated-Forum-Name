# pulling data from Name Field and Date Field. File name will be " Deposit Transfer - Name - Date.pdf "

var nameField = "Deposit Transfer - " + this.getField("NameField").value + " - " + this.getField("DateField") + ".pdf";
nameField = "/p/Housing/Business/Document Hub/Deposit Transfer/" + nameField
myTrustedSpecialTaskFunc(this, nameField);
